{
	"version": "2.0.0",
	"tasks": [
		{
			"label": "CMake configure (mingw-release)",
			"type": "shell",
			"command": "cmake --preset mingw-release",
			"problemMatcher": [
				"$gcc"
			],
			"group": "build"
		},
		{
			"label": "CMake build (mingw-release)",
			"type": "shell",
			"command": "cmake --build --preset mingw-release",
			"problemMatcher": [
				"$gcc"
			],
			"group": "build"
		}
	]
}